{
  "name": "@minstack/snippets",
  "license": "CC0-1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "test": "tsc && npm run lint --max-warnings=0 && jest --no-cache",
    "modernize": "npx npm-check-updates -i --format group",
    "release": "catch-uncommitted --exclude=.npmrc && lerna publish from-package --no-git-tag-version --no-push --yes",
    "bump": "lerna exec npm -- version patch"
  },
  "workspaces": [
    "*/"
  ],
  "devDependencies": {
    "@lerna-lite/exec": "^2.3.0",
    "@lerna-lite/publish": "^2.3.0",
    "@testing-library/react": "^14.0.0",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "catch-uncommitted": "^2.0.0",
    "eslint": "^8.40.0",
    "eslint-config-rational": "^2.0.20",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "node-fetch": "^2.6.9",
    "prettier": "^2.8.8",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "engineStrict": true
}
